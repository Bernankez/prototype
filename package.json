{
  "name": "@bernankez/prototype",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@9.1.3",
  "description": "Prototypes",
  "author": {
    "name": "Cole",
    "email": "bernankeic@gmail.com",
    "url": "https://github.com/Bernankez"
  },
  "license": "MIT",
  "homepage": "https://github.com/Bernankez/prototype#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bernankez/prototype.git"
  },
  "bugs": "https://github.com/Bernankez/prototype/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "dev": "vite serve playground",
    "dev:build": "vite build playground",
    "dev:preview": "vite preview playground",
    "build": "unbuild",
    "release": "bumpp && pnpm publish",
    "test": "vitest run",
    "test:watch": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "fix": "eslint . --fix"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.10.0",
    "markdown-it": "^14.1.0",
    "markdown-it-katex": "^2.0.3",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^1.4.0",
    "@bernankez/eslint-config-unocss": "^0.7.4",
    "@iconify-json/lucide": "^1.1.188",
    "@shikijs/markdown-it": "^1.6.0",
    "@shikijs/twoslash": "^1.6.0",
    "@types/markdown-it": "^13.0.8",
    "@types/node": "^20.12.12",
    "@unocss/eslint-plugin": "^0.60.3",
    "@unocss/reset": "^0.60.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "bumpp": "^9.4.1",
    "eslint": "^9.3.0",
    "eslint-plugin-format": "^0.1.1",
    "happy-dom": "^14.11.1",
    "lint-staged": "^15.2.5",
    "shiki": "^1.6.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "unocss": "^0.60.3",
    "vite": "^5.2.11",
    "vite-plugin-vue-devtools": "^7.2.1",
    "vitest": "^1.6.0",
    "vue": "^3.4.27",
    "vue-tsc": "^2.0.19"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
