{
  "name": "@bernankez/prototype",
  "type": "module",
  "version": "0.0.5",
  "packageManager": "pnpm@10.4.1",
  "description": "Prototypes",
  "author": {
    "name": "Cole",
    "email": "bernankeic@gmail.com",
    "url": "https://github.com/Bernankez"
  },
  "license": "MIT",
  "homepage": "https://github.com/Bernankez/prototype#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bernankez/prototype.git"
  },
  "bugs": "https://github.com/Bernankez/prototype/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "dev": "vite serve playground",
    "dev:build": "vite build playground",
    "dev:preview": "vite preview playground",
    "build": "unbuild",
    "release": "bumpp && pnpm publish",
    "test": "vitest run",
    "test:watch": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "fix": "eslint . --fix"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "dependencies": {
    "@vueuse/core": "^12.7.0",
    "markdown-it": "^14.1.0",
    "markdown-it-katex": "^2.0.3",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^3.0.0",
    "@bernankez/eslint-config-unocss": "^0.7.4",
    "@iconify-json/lucide": "^1.2.26",
    "@shikijs/markdown-it": "^3.0.0",
    "@shikijs/twoslash": "^3.0.0",
    "@types/markdown-it": "^13.0.9",
    "@types/node": "^22.13.5",
    "@unocss/eslint-plugin": "^66.0.0",
    "@unocss/reset": "^66.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-format": "^1.0.1",
    "happy-dom": "^17.1.8",
    "lint-staged": "^15.4.3",
    "shiki": "^3.0.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "unocss": "^66.0.0",
    "vite": "^6.1.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.7",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
